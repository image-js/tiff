{
  "name": "tiff",
  "version": "6.2.0",
  "description": "TIFF image decoder written entirely in JavaScript",
  "type": "module",
  "main": "lib/index.js",
  "module": "lib-esm/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "lib-esm",
    "src"
  ],
  "scripts": {
    "check-types": "tsc --noEmit",
    "clean": "rimraf lib lib-esm",
    "eslint": "eslint src",
    "eslint-fix": "npm run eslint -- --fix",
    "prepack": "npm run tsc",
    "prettier": "prettier --check src",
    "prettier-write": "prettier --write src",
    "test": "npm run test-only && npm run eslint && npm run prettier && npm run check-types",
    "test-only": "vitest run --coverage",
    "tsc": "npm run clean && npm run tsc-cjs && npm run tsc-esm",
    "tsc-cjs": "tsc --project tsconfig.cjs.json",
    "tsc-esm": "tsc --project tsconfig.esm.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/image-js/tiff.git"
  },
  "author": "MichaÃ«l Zasso",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/image-js/tiff/issues"
  },
  "homepage": "https://github.com/image-js/tiff#readme",
  "dependencies": {
    "iobuffer": "^5.4.0",
    "pako": "^2.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.13.10",
    "@types/pako": "^2.0.3",
    "@vitest/coverage-istanbul": "^3.0.8",
    "@vitest/coverage-v8": "^3.0.8",
    "eslint": "^9.22.0",
    "eslint-config-cheminfo-typescript": "^17.0.0",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.2",
    "vitest": "^3.0.8"
  }
}
